{% extends 'AppBundle::base.html.twig' %}

{% block body -%}
<h2>Banner (id: {{ entity. id }})</h2>

<p>
    <img src="{{ signedUrl }}" />
</p>

<h3>Logs</h3>

<ul class="logs">
    {% for log in entity.logs %}
    <li>
        {{ log.created|date('Y-m-d H:i:s') }}
    </li>
    {% endfor %}
</ul>
{% endblock %}
